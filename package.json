    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "tailwind:build": "postcss src/assets/css/tailwind.src.css -o src/assets/css/tailwind.css",
    "tailwind:watch": "postcss -w src/assets/css/tailwind.src.css -o src/assets/css/tailwind.css",
    "start:dev": "set NODE_ENV=development \nrun-p tailwind:watch start",
    "start": "react-scripts start",
    "prebuild": "set NODE_ENV=production \nnpm run tailwind:build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.5.1"
  }
